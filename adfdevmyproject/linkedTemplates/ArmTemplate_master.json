{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdevmyproject"
		},
		"LS_SINK_CSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SINK_CSV'"
		},
		"LS_TARGET_CSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_TARGET_CSV'"
		},
		"LS_source_CSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_source_CSV'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"LS_SINK_CSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdevmyproject.dfs.core.windows.net/"
		},
		"LS_TARGET_CSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnewproject001.dfs.core.windows.net/"
		},
		"LS_source_CSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnewproject001.dfs.core.windows.net/"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "VAIBHAV\\SQLEXPRESS"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2022"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdevmyproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SINK_CSV_accountKey": {
						"value": "[parameters('LS_SINK_CSV_accountKey')]"
					},
					"LS_TARGET_CSV_accountKey": {
						"value": "[parameters('LS_TARGET_CSV_accountKey')]"
					},
					"LS_source_CSV_accountKey": {
						"value": "[parameters('LS_source_CSV_accountKey')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"LS_SINK_CSV_properties_typeProperties_url": {
						"value": "[parameters('LS_SINK_CSV_properties_typeProperties_url')]"
					},
					"LS_TARGET_CSV_properties_typeProperties_url": {
						"value": "[parameters('LS_TARGET_CSV_properties_typeProperties_url')]"
					},
					"LS_source_CSV_properties_typeProperties_url": {
						"value": "[parameters('LS_source_CSV_properties_typeProperties_url')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}